{
  "name": "<%= name %>",
  "version": "0.0.0",
  "description": "<%= props.description %>",
  "authors": [{
    "name": "<%= props.authorName %>",
    "email": "<%= props.authorEmail %>"<% if (props.authorUrl) { %>,
    "homepage": "<%= props.authorUrl %>"<% } %>
  }],
  "main": "src/index.js",
  "files": [
    "src",
    "README.md"
  ],
  "keywords": [
    "<%= name %>",
    "<%= ComponentName %>"<% if(keywords.length){%><% for (var i = 0; i < keywords.length; i++) { %>,
    "<%= keywords[i] %>"<% }} %>
  ],
  "license": "MIT",
  "screenshots": "",
  "ignore": [
    "**/.*",
    "node_modules",
    "dist",
    "bower_components",
    "test",
    "tests"
  ]
}
