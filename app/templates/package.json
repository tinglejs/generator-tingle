{
  "name": "<%= name %>",
  "version": "0.0.0",
  "description": "<%= props.description %>",
  "repository": "https://github.com/tinglejs/<%= name %>.git",
  "author": {
    "name": "<%= props.authorName %>",
    "email": "<%= props.authorEmail %>"<% if (props.authorUrl) { %>,
    "url": "<%= props.authorUrl %>"<% } %>
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "<%= name %>,<%= ComponentName %>"<% for (var i = 0; i < keywords.length; i++) { %>,
    "<%= keywords[i] %>"<% } %>
  ],
  "dependencies": {
    "fastclick": "~1.0.6",
    "react": "^0.13.3"
  },
  "devDependencies": {
    "babel-core": "^5.2.17",
    "babel-loader": "^5.0.0",
    "browser-sync": "~1.9.1",
    "del": "^1.1.1",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "~1.5.1",
    "gulp-stylus": "~2.0.1",
    "gulp-uglify": "^1.2.0",
    "gulp-webpack": "~1.1.2",
    "webpack": "^1.8.4"
  },
  "scripts": {},
  "license": "MIT"
}
