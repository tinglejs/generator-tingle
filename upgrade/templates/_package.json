{
  "name": "<%= name %>",
  "version": "0.0.0",
  "description": "<%= props.description %>",
  "repository": "https://github.com/tinglejs/<%= name %>.git",
  "author": {
    "name": "<%= props.authorName %>",
    "email": "<%= props.authorEmail %>"<% if (props.authorUrl) { %>,
    "url": "<%= props.authorUrl %>"<% } %>
  },
  "main": "src/index.js",
  "files": [
    "src",
    "README.md"
  ],
  "keywords": [
    "<%= name %>,<%= ComponentName %>"<% if(keywords.length){%><% for (var i = 0; i < keywords.length; i++) { %>,
    "<%= keywords[i] %>"<% }} %>
  ],
  "dependencies": {
    "classnames": "^2.1.2",
    "fastclick": "^1.0.6",
    "react": "^0.13.3",
    "tingle-context": "~0.0.3",
    "tingle-style": "~0.0.1",
    "zepto": "0.0.4"
  },
  "devDependencies": {
    "babel-core": "^5.8.19",
    "babel-loader": "^5.3.2",
    "gulp": "^3.9.0",
    "tingle-dev-tools": "latest"
  },
  "scripts": {
    "start": "gulp server"
  },
  "license": "MIT"
}
